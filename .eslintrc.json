{
  "env": {
    "es2021": true,
    "node": true
  },
  "extends": [
    "airbnb-base"
  ],
  "parserOptions": {
    "ecmaVersion": 13,
    "sourceType": "module"
  },
  "rules": {
    "import/first": "off",
    "max-len": ["error", { "code": 120 }],
    "no-console": "off",
    "no-underscore-dangle": ["error", { "allow": ["_id"] }]
  }
}
